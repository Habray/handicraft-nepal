r\n\r\n  return (\r\n    <>\r\n    <div className=\"container\">\r\n        \r\n        {/* we use loadingcard to make more fancy looking while loading our card */}\r\n        {loading ? (\r\n          <LoadingCard count={3} />\r\n        ) : (\r\n          <div className=\"row\">\r\n\r\n            {/* mapping through each product and show it as card */}\r\n            {products.map((product) => (\r\n              <div key={product._id} className=\"col-md-4\">\r\n                <ProductCard product={product} />\r\n              </div>\r\n            ))}\r\n          </div>\r\n        )}\r\n      </div>\r\n\r\n      {/* implemeting pagination */}\r\n      <div className=\"row\">\r\n        <nav className=\"col-md-4 offset-md-4 text-center pt-5 p-3\">\r\n          <Pagination\r\n            current={page}\r\n            total={(productsCount / 3) * 10}\r\n            onChange={(value) => setPage(value)}\r\n          />\r\n        </nav>\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n  \r\n  export default BestSellers;","D:\\projects\\Handicraft_Nepal_2\\client\\src\\pages\\Product.js",["640","641"],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\cards\\SingleProduct.js",["642","643","644","645","646","647"],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\cards\\ProductListItems.js",[],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\modal\\RatingModal.js",["648"],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\functions\\rating.js",[],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\category\\CategoryList.js",[],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\pages\\category\\CategoryHome.js",["649","650","651"],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\sub\\SubList.js",[],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\pages\\sub\\SubHome.js",["652"],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\reducers\\searchReducer.js",[],"D:\\projects\\Handicraft_Nepal_2\\client\\src\\components\\forms\\Search.js",[]