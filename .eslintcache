        <ProductCard product={p} />\n          </div>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nexport default SubHome;\n","D:\\projects\\Handicraft_Nepal_1\\client\\src\\pages\\admin\\coupon\\CreateCouponPage.js",["696"],"import React, { useState, useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { toast } from \"react-toastify\";\nimport DatePicker from \"react-datepicker\";\nimport {\n  getCoupons,\n  removeCoupon,\n  createCoupon,\n} from \"../../../functions/coupon\";\nimport \"react-datepicker/dist/react-datepicker.css\";\nimport { DeleteOutlined } from \"@ant-design/icons\";\nimport AdminNav from \"../../../components/nav/AdminNav\";\n\nconst CreateCouponPage = () => {\n\n  // Name\n  const [name, setName] = useState(\"\");\n\n  // Exipry Date\n  const [expiry, setExpiry] = useState(\"\");\n\n  // Discount Amount\n  const [discount, setDiscount] = useState(\"\");\n\n  // Loading \n  const [loading, setLoading] = useState(\"\");\n\n  // Coupons of DB\n  const [coupons, setCoupons] = useState([]);\n\n  // redux\n  const { user } = useSelector((state) => ({ ...state }));\n\n  useEffect(() => {\n    loadAllCoupons();\n  }, []);\n\n  // list Coupon\n  const loadAllCoupons = () => getCoupons().then((res) => setCoupons(res.data));\n\n  // Create Coupon\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    setLoading(true);\n    // console.table(name, expiry, discount);\n    createCoupon({ name, expiry, discount }, user.token)\n      .then((res) => {\n        setLoading(false);\n        loadAllCoupons(); // load all coupons\n        setName(\"\");\n        setDiscount(\"\");\n        setExpiry(\"\");\n        toast.success(`\"${res.data.name}\" is created`);\n      })\n      .catch((err) => console.log(\"create coupon err\", err));\n  };\n\n  // Remove\n  const handleRemove = (couponId) => {\n    if (window.confirm(\"Delete?\")) {\n      setLoading(true);\n      removeCoupon(couponId, user.token)\n        .then((res) => {\n          loadAllCoupons(); // load all coupons\n          setLoading(false);\n          toast.error(`Coupon \"${res.data.name}\" deleted`);\n        })\n        .catch((err) => console.log(err));\n    }\n  };\n\n  return (\n    <div className=\"container-fluid\">\n      <div className=\"row\">\n\n        {/* Admin Sidebar */}\n        <div className=\"col-md-2\">\n          <AdminNav />\n        </div>\n\n        {/* Coupon Components */}\n        {/* Title */}\n        <div className=\"col-md-10\">\n          {loading ? (\n            <h4 className=\"text-danger\">Loading...</h4>\n          ) : (\n            <h4>Coupon</h4>\n          )}\n\n          <form onSubmit={handleSubmit}>\n\n            {/* Create */}\n            <div className=\"form-group\">\n              <label className=\"text-muted\">Name</label>\n              <input\n                type=\"text\"\n                className=\"form-control\"\n                onChange={(e) => setName(e.target.value)}\n                value={name}\n                autoFocus\n                required\n              />\n            </div>\n\n            {/* Discount */}\n            <div className=\"form-group\">\n              <label className=\"text-muted\">Discount %</label>\n              <input\n                type=\"text\"\n                className=\"form-control\"\n                onChange={(e) => setDiscount(e.target.value)}\n                value={discount}\n                required\n              />\n            </div>\n\n            {/* Exipry Date */}\n            <div className=\"form-group\">\n              <label className=\"text-muted\">Expiry</label>\n              <br />\n              <DatePicker\n                className=\"form-control\"\n                selected={new Date()}\n                value={expiry}\n                onChange={(date) => setExpiry(date)}\n                required\n              />\n            </div>\n\n            {/* Save Button */}\n            <button className=\"btn btn-outline-primary\">Save</button>\n          </form>\n\n          <br />\n\n          {/* Show Existing Coupons */}\n          <h4>{coupons.length} Coupons</h4>\n\n          <table className=\"table table-bordered\">\n            {/* title of coupon table */}\n            <thead className=\"thead-light\">\n              <tr>\n                <th scope=\"col\">Name</th>\n                <th scope=\"col\">Expiry</th>\n                <th scope=\"col\">Discount</th>\n                <th scope=\"col\">Action</th>\n              </tr>\n            </thead>\n\n            {/* coupon table datas */}\n            <tbody>\n              {/* map though each coupone in DB */}\n              {coupons.map((c) => (\n                <tr key={c._id}>\n                  <td>{c.name}</td>\n                  <td>{new Date(c.expiry).toLocaleDateString()}</td>\n                  <td>{c.discount}%</td>\n                  <td>\n                    <DeleteOutlined\n                      onClick={() => handleRemove(c._id)}\n                      className=\"text-danger pointer\"\n                    />\n                  </td>\n                </tr>\n              ))}\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default CreateCouponPage;\n","D:\\projects\\Handicraft_Nepal_1\\client\\src\\pages\\admin\\product\\AllProducts.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\routes\\UserRoute.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\routes\\AdminRoute.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\drawer\\SideDrawer.js",["697","698","699"],"import React from \"react\";\nimport { Drawer, Button } from \"antd\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { Link } from \"react-router-dom\";\nimport defaultImage from '../../images/defaultImage.jpg'\n\nconst SideDrawer = () => {\n  const dispatch = useDispatch();\n  const { drawer, cart } = useSelector((state) => ({ ...state }));\n\n  const imageStyle = {\n    width: \"100%\",\n    height: \"100px\",\n    objectFit: \"contain\",\n  };\n\n  return (\n    <Drawer\n      className=\"text-center\"\n      title={`Cart / ${cart.length} Product`}\n      placement=\"right\"\n      closable={false}\n      onClose={() => {\n\n        // save redux state to close the drawer\n        dispatch({\n          type: \"SET_VISIBLE\",\n          payload: false,\n        });\n      }}\n      visible={drawer}\n    >\n\n      {/* Cart Products */}\n      {cart.map((p) => (\n        <div key={p._id} className=\"row\">\n          <div className=\"col\">\n            {p.images[0] ? (\n              <>\n                <img src={p.images[0].url} style={imageStyle} />\n                <p className=\"text-center bg-secondary text-light\">\n                  {p.title} x {p.count}\n                </p>\n              </>\n            ) : (\n              <>\n                {/* If there is no product image */}\n                <img src={defaultImage} style={imageStyle} />\n                <p className=\"text-center bg-secondary text-light\">\n                  {p.title} x {p.count}\n                </p>\n              </>\n            )}\n          </div>\n        </div>\n      ))}\n\n      {/* Goto Cart */}\n      <Link to=\"/cart\">\n        <button\n          onClick={() =>\n            dispatch({\n              type: \"SET_VISIBLE\",\n              payload: false,\n            })\n          }\n          className=\"text-center btn btn-primary btn-raised btn-block\"\n        >\n          Go To Cart\n        </button>\n      </Link>\n    </Drawer>\n  );\n};\n\nexport default SideDrawer;\n","D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\nav\\Header.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\functions\\product.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\functions\\category.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\nav\\UserNav.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\StripeCheckout.js",["700","701","702"],"import React, { useState, useEffect } from \"react\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { createPaymentIntent } from \"../functions/stripe\";\nimport { Link } from \"react-router-dom\";\nimport { Card } from \"antd\";\nimport { DollarOutlined, CheckOutlined, SwapOutlined } from \"@ant-design/icons\";\nimport cardImage from '../images/creditcard.jpg'\nimport { createOrder, emptyUserCart } from \"../functions/user\";\n\nconst StripeCheckout = ({ history }) => {\n\n  // access dispatch\n  const dispatch = useDispatch();\n\n  // redux\n  const { user, coupon } = useSelector((state) => ({ ...state }));\n\n  // state variables\n  const [succeeded, setSucceeded] = useState(false);\n  const [error, setError] = useState(null);\n  const [processing, setProcessing] = useState(\"\");\n  const [disabled, setDisabled] = useState(true);\n  const [clientSecret, setClientSecret] = useState(\"\");\n\n  // after coupon applied\n  const [cartTotal, setCartTotal] = useState(0);\n  const [totalAfterDiscount, setTotalAfterDiscount] = useState(0);\n  const [payable, setPayable] = useState(0);\n\n  // implementing stripe hooks\n  const stripe = useStripe();\n  const elements = useElements();\n\n  useEffect(() => {\n    createPaymentIntent(user.token, coupon).then((res) => {\n      console.log(\"create payment intent\", res.data);\n      setClientSecret(res.data.clientSecret);\n      // additional response received on successful payment\n      setCartTotal(res.data.cartTotal);\n      setTotalAfterDiscount(res.data.totalAfterDiscount);\n      setPayable(res.data.payable);\n    });\n  }, []);\n\n  // for conform payment \n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setProcessing(true);  // showing processing\n\n    // request stripe for conform card payment\n    const payload = await stripe.confirmCardPayment(clientSecret, {\n      payment_method: {\n        card: elements.getElement(CardElement),\n        billing_details: {\n          name: e.target.name.value,\n        },\n      },\n    });\n\n    // error handeling\n    if (payload.error) {\n      setError(`Payment failed ${payload.error.message}`);\n      setProcessing(false);   // stop showing processing\n    } else {\n      // here you get result after successful payment\n      // create order and save in database for admin to process\n      createOrder(payload, user.token).then((res) => {\n        if (res.data.ok) {\n          // empty cart from local storage\n          if (typeof window !== \"undefined\") localStorage.removeItem(\"cart\");\n          // empty cart from redux\n          dispatch({\n            type: \"ADD_TO_CART\",\n            payload: [],\n          });\n          // reset coupon to false\n          dispatch({\n            type: \"COUPON_APPLIED\",\n            payload: false,\n          });\n          // empty cart from database. we use same endpoint as we use to empty cart before payment\n          emptyUserCart(user.token);\n        }\n      });\n      // empty user cart from redux store and local storage\n      console.log(JSON.stringify(payload, null, 4));\n      setError(null);         // reset error\n      setProcessing(false);   // stop showing processing\n      setSucceeded(true);     \n    }\n  };\n\n  // input credit card details\n  const handleChange = async (e) => {\n    // listen for changes in the card element\n    // and display any errors as the custoemr types their card details\n    setDisabled(e.empty); // disable pay button if errors\n    setError(e.error ? e.error.message : \"\"); // show error message\n  };\n\n  const cartStyle = {\n    style: {\n      base: {\n        color: \"#32325d\",\n        fontFamily: \"Arial, sans-serif\",\n        fontSmoothing: \"antialiased\",\n        fontSize: \"16px\",\n        \"::placeholder\": {\n          color: \"#32325d\",\n        },\n      },\n      invalid: {\n        color: \"#fa755a\",\n        iconColor: \"#fa755a\",\n      },\n    },\n  };\n\n  return (\n    <>\n      {/* Image and Price Details */}\n      {/* Message for applied coupon */}\n      {!succeeded && (\n        <div>\n          {coupon && totalAfterDiscount !== undefined ? (\n            <p className=\"alert alert-success\">{`Total after discount: $${totalAfterDiscount}`}</p>\n          ) : (\n            <p className=\"alert alert-danger\">No coupon applied</p>\n          )}\n        </div>\n      )}\n      <div className=\"text-center pb-5\">\n        <Card\n          cover={\n            <img\n              src={cardImage}\n              style={{\n                height: \"230px\",\n                objectFit: \"cover\",\n                marginBottom: \"-50px\",\n              }}\n            />\n          }\n          actions={[\n            <>\n              <DollarOutlined className=\"text-info\" /> <br /> Total: Rs. {cartTotal}\n            </>,\n            <>\n              <CheckOutlined className=\"text-info\" /> <br /> Total payable : $\n              {(payable / 100).toFixed(2)}\n            </>,\n          ]}\n        />\n      </div>\n\n      {/* Credit Card details Section */}\n      <form id=\"payment-form\" className=\"stripe-form\" onSubmit={handleSubmit}>\n        <CardElement\n          id=\"card-element\"\n          options={cartStyle}\n          onChange={handleChange}\n        />\n\n        {/* Pay button */}\n        <button\n          className=\"stripe-button\"\n          disabled={processing || disabled || succeeded}\n        >\n          <span id=\"button-text\">\n            {processing ? <div className=\"spinner\" id=\"spinner\"></div> : \"Pay\"}\n          </span>\n        </button>\n        <br />\n\n        {/* Error Handeling */}\n        {error && (\n          <div className=\"card-error\" role=\"alert\">\n            {error}\n          </div>\n        )}\n        <br />\n\n        {/* Showing Success Message */}\n        <p className={succeeded ? \"result-message\" : \"result-message hidden\"}>\n          Payment Successful.{\" \"}\n          <Link to=\"/user/history\">See it in your purchase history.</Link>\n        </p>\n      </form>\n    </>\n  );\n};\n\nexport default StripeCheckout;\n","D:\\projects\\Handicraft_Nepal_1\\client\\src\\functions\\user.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\functions\\sub.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\functions\\admin.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\nav\\AdminNav.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\cards\\Jumbotron.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\forms\\FileUpload.js",[],"D:\\projects\\Handicraft_Nepal_1\\client\\src\\components\\forms\\ProductCreateForm.js",["703","704","705","706","707"],"import React from 'react'\r\nimport {Select} from 'antd'\r\n\r\nconst {Option} = Select\r\n\r\nconst ProductCreateForm = ({\r\n    handleChange,\r\n    handleSubmit, \r\n    values, \r\n    setValues,\r\n    handleCategoryChange, \r\n    subOptions,\r\n    showSub,\r\n}) => { \r\n\r\n    // destructure\r\n    const {\r\n        title,\r\n        description,\r\n        price,\r\n        category,\r\n        subs,\r\n        shipping,\r\n        quantity,\r\n        images,\r\n        colors,\r\n        color,\r\n        categories,\r\n        brands,\r\n        brand,\r\n    } = values\r\n\r\n\r\n    return (\r\n        <form onSubmit={handleSubmit}>\r\n\r\n            {/* For Title */}\r\n            <div className='form-group'>\r\n                <label>Title</label>\r\n                <input \r\n                    type='text' \r\n                    name='title' \r\n                    className='form-control' \r\n                    value={title} \r\n                    onChange={handleChange}\r\n                />\r\